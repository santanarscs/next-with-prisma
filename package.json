{
  "name": "next-with-prisma",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "generate": "yarn run generate:prisma && yarn run generate:nexus",
    "generate:prisma": "prisma generate",
    "generate:nexus": "ts-node --transpile-only -P nexus.tsconfig.json pages/api",
    "migrate": "yarn migrate:save && yarn migrate:up",
    "migrate:save": "npx prisma migrate save --experimental",
    "migrate:up": "npx prisma migrate up --experimental"
  },
  "dependencies": {
    "@nexus/schema": "^0.14.0",
    "@prisma/client": "2.3.0",
    "formik": "^2.1.5",
    "graphql": "^15.3.0",
    "next": "^11.1.0",
    "nexus": "^0.25.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@prisma/cli": "2.3.0",
    "@types/node": "^14.0.27",
    "@types/react": "^16.9.43",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7"
  }
}
